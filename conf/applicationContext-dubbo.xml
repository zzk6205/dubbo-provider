<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-4.2.xsd
	   					http://www.springframework.org/schema/context
       					http://www.springframework.org/schema/context/spring-context-4.2.xsd
       					http://www.springframework.org/schema/mvc
       					http://www.springframework.org/schema/mvc/spring-mvc-4.2.xsd
       					http://code.alibabatech.com/schema/dubbo
       					http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<context:property-placeholder location="classpath:base.properties" ignore-resource-not-found="true" ignore-unresolvable="true" />

	<dubbo:application name="${dubbo.application.name}" />

	<dubbo:protocol name="dubbo" port="${dubbo.protocol.port}" />

	<dubbo:registry address="${dubbo.registry.address}" check="false" subscribe="false" register="" />

	<dubbo:service interface="net.web.business.service.ITenantService" ref="tenantService" />

</beans>